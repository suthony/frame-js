{
  "ID": "do_External",
  "IsContainer": "0",
  "Type": "SM",
  "Name": "启动外部应用",
  "Version": "Internal",
  "Description": "启动外部应用或系统的一些应用，比如浏览器，短信等",
  "Icon": "https://deviceone.blob.core.chinacloudapi.cn/store/R/20150624/11c193e8-d7d4-4200-b0ee-9d6b81c1db21.png",
  "IOS": {
    "Support": 1,
    "MinVersion": "iOS7.0"
  },
  "Android": {
    "Support": 1,
    "MinVersion": "4.0"
  },
  "Windows": {
    "Support": 1,
    "MinVersion": "8.1"
  },
  "WindowsPhone": {
    "Support": 1,
    "MinVersion": "8.1"
  },
  "Html": {
    "Support": 1,
    "MinVersion": "5.0"
  },
  "Properties": [],
  "Events": [],
  "syncMethodes": [
    {
      "ID": "installApp",
      "Name": "安装app",
      "Description": "执行这个方法会打开系统的安装界面安装data://目录下的一个apk文件，仅支持android平台",
      "ReturnType": "",
      "ReturnDesc": "返回true或false",
      "Paras": [
        {
          "ID": "path",
          "Name": "安装包存放地址",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "只支持data://目录"
        }
      ]
    },
    {
      "ID": "openApp",
      "Name": "启动其他应用",
      "Description": "如果唤醒id对应的应用不存在，返回失败",
      "ReturnType": "Bool",
      "ReturnDesc": "返回true或false",
      "Paras": [
        {
          "ID": "wakeupid",
          "Name": "唤醒id",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "其他应用被唤醒的唯一标识。不同的手机操作系统唤醒机制不一样，唤醒id的格式也不一样。我们会有专门的文档介绍。"
        },
        {
          "ID": "data",
          "Name": "传递数据",
          "Type": "Node",
          "Required": 0,
          "DefaultValue": "",
          "Description": "打开其他应用可以传递一些数据，数据的格式是JSON的键值对。"
        }
      ]
    },
    {
      "ID": "openContact",
      "Name": "打开系统通讯录界面",
      "Description": "",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": []
    },
    {
      "ID": "openDial",
      "Name": "打开拨号界面",
      "Description": "打开系统拨号界面",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "number",
          "Name": "号码",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "用户传入的号码可以显示到拨号界面"
        }
      ]
    },
    {
      "ID": "openFile",
      "Name": "打开外部文档",
      "Description": "android需要先在手机上安装打开相应文件的第三方软件",
      "ReturnType": "Bool",
      "ReturnDesc": "成功返回true，失败返回false",
      "Paras": [
        {
          "ID": "path",
          "Name": "文档存放地址",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "只支持data://目录"
        }
      ]
    },
    {
      "ID": "openMail",
      "Name": "打开发送邮件界面",
      "Description": "",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "to",
          "Name": "收件箱地址",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "收件箱地址，非空"
        },
        {
          "ID": "subject",
          "Name": "邮件标题",
          "Type": "String",
          "Required": 0,
          "DefaultValue": "",
          "Description": "邮件标题"
        },
        {
          "ID": "body",
          "Name": "邮件正文",
          "Type": "String",
          "Required": 0,
          "DefaultValue": "",
          "Description": "邮件正文"
        }
      ]
    },
    {
      "ID": "openSMS",
      "Name": "打开发送短信界面",
      "Description": "",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "number",
          "Name": "手机号",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "手机号"
        },
        {
          "ID": "body",
          "Name": "内容",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "内容"
        }
      ]
    },
    {
      "ID": "openURL",
      "Name": "调用系统默认浏览器",
      "Description": "手机缺省都有自带的浏览器，这个方法可以调用这个浏览器打开指定的url",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "url",
          "Name": "url",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "操作系统浏览器打开的url地址"
        }
      ]
    }
  ],
  "asyncMethodes": []
}